     RowBox[{"(*", 
      RowBox[{
       RowBox[{"1", "/", "sec"}], ",", " ", 
       RowBox[{"reaction", " ", "rate", " ", "of", " ", "decay"}]}], "*)"}], 
     "\[IndentingNewLine]", 
