9fbad5e4a2c7"],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"v", " ", 
    RowBox[{
     SuperscriptBox["c", "\[Prime]",
      MultilineFunction->None], "(", "t", ")"}]}], "\[LongEqual]", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"h", " ", "k", " ", 
      SuperscriptBox["sl", "2"], " ", 
      RowBox[{"c", "(", "t", ")"}]}]}], "-", 
    RowBox[{"h", " ", "sl", " ", "v", " ", 
     RowBox[{"c", "(", "t", ")"}]}], "+", "in"}]}], 
  TraditionalForm]], "Output",
 CellChangeTimes->{{3.714144833729604*^9, 3.7141448524853086`*^9}, 
