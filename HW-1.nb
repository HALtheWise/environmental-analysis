425d65b1dade"],

Cell[BoxData[
 RowBox[{
  RowBox[{"consts", " ", "=", " ", 
   RowBox[{"<|", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"sl", "\[Rule]", "100000"}], ",", 
     RowBox[{"(*", 
      RowBox[{"meters", ",", " ", 
       RowBox[{"side", " ", "length", " ", "of", " ", "cube"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"h", "\[Rule]", "1000"}], ",", 
