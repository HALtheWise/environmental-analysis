ecf44c2d631b"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(", 
   RowBox[{"eqn2", "=", 
    RowBox[{"(", 
     RowBox[{"eqn", "/.", 
      RowBox[{"<|", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"c", "'"}], "[", "t", "]"}], "\[Rule]", "0"}], ",", 
        RowBox[{
         RowBox[{"c", "[", "t", "]"}], "\[Rule]", "c\[Infinity]"}]}], 
       "|>"}]}], ")"}]}], ")"}], "//", "TraditionalForm"}]], "Code",
 CellChangeTimes->{{3.7141448089659653`*^9, 3.7141448604807777`*^9}, {
