      RowBox[{"meters", ",", " ", 
       RowBox[{"height", " ", "of", " ", "cube"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"v", "\[Rule]", "4"}], ",", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"m", "/", "s"}], ",", " ", 
       RowBox[{"wind", " ", "velocity"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"k", "\[Rule]", 
      RowBox[{"2", "*", 
       RowBox[{"(", 
